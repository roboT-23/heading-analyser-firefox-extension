{
  "manifest_version": 2,
  "name": "Heading Analyzer",
  "version": "1.0",
  "description": "Analyzes and displays the headings on the current page.",
  "permissions": [
    "activeTab"
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  }
}
